{
	"name": "SearchNonTLD",
	"version": "0.0.1",
	"description": "Make non-valid TLD searching",
	"homepage_url": "https://github.com/phu54321/SearchNonTLD",
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "your-extension-name@your-domain.com",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
  "permissions": ["webNavigation", "storage", "search", "tabs"],
	"background": {
		"scripts": [
			"browser-polyfill.min.js",
			"background.js"
		]
	}
}
